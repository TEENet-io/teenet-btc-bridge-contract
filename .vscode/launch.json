{
    "version": "0.2.0",
    "configurations": [
        {
			"name": "Mocha Current File",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
			"args": [
				"--no-timeouts",
				"${file}"
			],
			"internalConsoleOptions": "openOnSessionStart",
			"console": "integratedTerminal",
			"sourceMaps": true
		}
    ],
    "compounds": []
}